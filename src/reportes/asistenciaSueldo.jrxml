	<queryString>
		<![CDATA[SELECT
     planilla_pago_sueldo.`idPago` AS planilla_pago_sueldo_idPago,
     planilla_pago_sueldo.`codigoEmpleado` AS planilla_pago_sueldo_codigoEmpleado,
